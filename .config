########################################
# LuCI 管理界面
########################################
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-app-passwall2=y

########################################
# Lua 支持 (lucihttp 依赖)
########################################
CONFIG_PACKAGE_lua=y
CONFIG_PACKAGE_liblua=y
CONFIG_PACKAGE_liblua-dev=y
CONFIG_PACKAGE_lucihttp=y

########################################
# PassWall2 常用代理依赖
########################################
